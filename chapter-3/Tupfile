include_rules
: Geometry.hs |> ghc %f |> %B.hi %B.o {geometry}
: Problem.hs |> ghc %f |> %B.hi %B.o {problem}
: UniformCost.hs | {problem} |> ghc %f |> %B.hi %B.o {uniformcost}
: BreadthFirst.hs | {problem} |> ghc %f |> %B.hi %B.o {bfs}
: solve.hs | {geometry} {problem} {bfs} {uniformcost} |> ghc -o %o %f ; rm -f %B.hi %B.o |> %B
